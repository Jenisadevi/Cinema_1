  </span><span><span class="hljs-keyword">const</span></span><span> movies = </span><span><span class="hljs-title function_">getAllMovies</span></span><span>().</span><span><span class="hljs-title function_">map</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">m</span></span></span><span>) =&gt; ({
