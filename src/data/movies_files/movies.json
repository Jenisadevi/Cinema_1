      result = result.</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">m</span></span></span><span>) =&gt;
        m.</span><span><span class="hljs-property">genres</span></span><span>.</span><span><span class="hljs-title function_">some</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">gen</span></span></span><span>) =&gt; gen.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>() === g),
      );
    }

    result.</span><span><span class="hljs-title function_">sort</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; {
