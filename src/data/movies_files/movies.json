</span><span><span class="hljs-keyword">type</span></span><span> </span><span><span class="hljs-title class_">Props</span></span><span> = {
  </span><span><span class="hljs-attr">movies</span></span><span>: </span><span><span class="hljs-title class_">Movie</span></span><span>[];
  </span><span><span class="hljs-attr">genres</span></span><span>: </span><span><span class="hljs-built_in">string</span></span><span>[];
};

</span><span><span class="hljs-keyword">type</span></span><span> </span><span><span class="hljs-title class_">SortOption</span></span><span> = </span><span><span class="hljs-string">'rating-desc'</span></span><span> | </span><span><span class="hljs-string">'year-desc'</span></span><span> | </span><span><span class="hljs-string">'title-asc'</span></span><span>;

</span><span><span class="hljs-keyword">export</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">MoviesList</span></span><span>(</span><span><span class="hljs-params">{ movies, genres }: Props</span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> [query, setQuery] = </span><span><span class="hljs-title function_">useState</span></span><span>(</span><span><span class="hljs-string">''</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> [genre, setGenre] = useState&lt;</span><span><span class="hljs-built_in">string</span></span><span>&gt;(</span><span><span class="hljs-string">'all'</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> [sort, setSort] = useState&lt;</span><span><span class="hljs-title class_">SortOption</span></span><span>&gt;(</span><span><span class="hljs-string">'rating-desc'</span></span><span>);

  </span><span><span class="hljs-keyword">const</span></span><span> filtered = </span><span><span class="hljs-title function_">useMemo</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">let</span></span><span> result = [...movies];

    </span><span><span class="hljs-keyword">if</span></span><span> (query.</span><span><span class="hljs-title function_">trim</span></span><span>()) {
      </span><span><span class="hljs-keyword">const</span></span><span> q = query.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>();
      result = result.</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">m</span></span></span><span>) =&gt; m.</span><span><span class="hljs-property">title</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>().</span><span><span class="hljs-title function_">includes</span></span><span>(q));
    }

    </span><span><span class="hljs-keyword">if</span></span><span> (genre !== </span><span><span class="hljs-string">'all'</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> g = genre.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>();
      result = result.</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">m</span></span></span><span>) =&gt;
        m.</span><span><span class="hljs-property">genres</span></span><span>.</span><span><span class="hljs-title function_">some</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">gen</span></span></span><span>) =&gt; gen.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>() === g),
      );
    }

    result.</span><span><span class="hljs-title function_">sort</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">a, b</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">if</span></span><span> (sort === </span><span><span class="hljs-string">'rating-desc'</span></span><span>) </span><span><span class="hljs-keyword">return</span></span><span> b.</span><span><span class="hljs-property">rating</span></span><span> - a.</span><span><span class="hljs-property">rating</span></span><span>;
      </span><span><span class="hljs-keyword">if</span></span><span> (sort === </span><span><span class="hljs-string">'year-desc'</span></span><span>)
        </span><span><span class="hljs-keyword">return</span></span><span> (
          </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Date</span></span><span>(b.</span><span><span class="hljs-property">releaseDate</span></span><span>).</span><span><span class="hljs-title function_">getTime</span></span><span>() -
          </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Date</span></span><span>(a.</span><span><span class="hljs-property">releaseDate</span></span><span>).</span><span><span class="hljs-title function_">getTime</span></span><span>()
        );
      </span><span><span class="hljs-keyword">return</span></span><span> a.</span><span><span class="hljs-property">title</span></span><span>.</span><span><span class="hljs-title function_">localeCompare</span></span><span>(b.</span><span><span class="hljs-property">title</span></span><span>);
    });

    </span><span><span class="hljs-keyword">return</span></span><span> result;
  }, [movies, query, genre, sort]);

  </span><span><span class="hljs-keyword">return</span></span><span> (
    </span><span><span class="language-xml"><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"space-y-6"</span></span><span>&gt;
      {/* Controls */}
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"flex flex-col gap-3 rounded-2xl border border-slate-800 bg-slate-900/40 p-4 sm:flex-row sm:items-center sm:justify-between"</span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"flex flex-1 gap-2"</span></span><span>&gt;
          </span><span><span class="hljs-tag">&lt;<span class="hljs-name">input</span></span></span><span>
            </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"search"</span></span><span>
            </span><span><span class="hljs-attr">placeholder</span></span><span>=</span><span><span class="hljs-string">"Search by title..."</span></span><span>
            </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">{query}</span></span><span>
            </span><span><span class="hljs-attr">onChange</span></span><span>=</span><span><span class="hljs-string">{(e)</span></span><span> =&gt; setQuery(e.target.value)}
            className="w-full rounded-xl border border-slate-800 bg-slate-900 px-3 py-2 text-xs text-slate-100 placeholder:text-slate-500 focus:border-indigo-500 focus:outline-none focus:ring-2 focus:ring-indigo-500/30"
          /&gt;
        </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;

        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"flex flex-wrap gap-2 text-xs"</span></span><span>&gt;
          </span><span><span class="hljs-tag">&lt;<span class="hljs-name">select</span></span></span><span>
            </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">{genre}</span></span><span>
            </span><span><span class="hljs-attr">onChange</span></span><span>=</span><span><span class="hljs-string">{(e)</span></span><span> =&gt; setGenre(e.target.value)}
            className="rounded-xl border border-slate-800 bg-slate-900 px-3 py-2 focus:border-indigo-500 focus:outline-none focus:ring-2 focus:ring-indigo-500/30"
          &gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"all"</span></span><span>&gt;All genres</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
            {genres.map((g) =&gt; (
              </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">key</span></span><span>=</span><span><span class="hljs-string">{g}</span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">{g}</span></span><span>&gt;
                {g}
              </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
            ))}
          </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">select</span></span></span><span>&gt;

          </span><span><span class="hljs-tag">&lt;<span class="hljs-name">select</span></span></span><span>
            </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">{sort}</span></span><span>
            </span><span><span class="hljs-attr">onChange</span></span><span>=</span><span><span class="hljs-string">{(e)</span></span><span> =&gt; setSort(e.target.value as SortOption)}
            className="rounded-xl border border-slate-800 bg-slate-900 px-3 py-2 focus:border-indigo-500 focus:outline-none focus:ring-2 focus:ring-indigo-500/30"
          &gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"rating-desc"</span></span><span>&gt;Top rated</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"year-desc"</span></span><span>&gt;Newest</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"title-asc"</span></span><span>&gt;Title AZ</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
          </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">select</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;

      {/* Grid */}
      {filtered.length === 0 ? (
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">p</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"text-sm text-slate-300"</span></span><span>&gt;
          No movies match your filters yet. Try adjusting your search.
