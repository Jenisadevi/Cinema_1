          &gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"rating-desc"</span></span><span>&gt;Top rated</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"year-desc"</span></span><span>&gt;Newest</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
            </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"title-asc"</span></span><span>&gt;Title AZ</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
          </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">select</span></span></span><span>&gt;
