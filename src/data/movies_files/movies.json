</span><span><span class="hljs-keyword">import</span></span><span> { </span><span><span class="hljs-title class_">MovieCard</span></span><span> } </span><span><span class="hljs-keyword">from</span></span><span> </span><span><span class="hljs-string">'./movie-card'</span></span><span>;

</span><span><span class="hljs-keyword">type</span></span><span> </span><span><span class="hljs-title class_">Props</span></span><span> = {
