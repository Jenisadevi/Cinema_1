  </span><span><span class="hljs-keyword">const</span></span><span> [query, setQuery] = </span><span><span class="hljs-title function_">useState</span></span><span>(</span><span><span class="hljs-string">''</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> [genre, setGenre] = useState&lt;</span><span><span class="hljs-built_in">string</span></span><span>&gt;(</span><span><span class="hljs-string">'all'</span></span><span>);
