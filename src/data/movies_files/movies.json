</span><span><span class="hljs-keyword">export</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">getMovieBySlug</span></span><span>(</span><span><span class="hljs-params">slug: <span class="hljs-built_in">string</span></span></span><span>): </span><span><span class="hljs-title class_">Movie</span></span><span> | </span><span><span class="hljs-literal">undefined</span></span><span> {
