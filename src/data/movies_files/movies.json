  </span><span><span class="hljs-keyword">if</span></span><span> (movies.</span><span><span class="hljs-property">length</span></span><span> === </span><span><span class="hljs-number">0</span></span><span>) {
    </span><span><span class="hljs-title function_">notFound</span></span><span>();
  }

  </span><span><span class="hljs-keyword">const</span></span><span> prettyGenre = </span><span><span class="hljs-built_in">decodeURIComponent</span></span><span>(genreParam);

  </span><span><span class="hljs-keyword">return</span></span><span> (
    </span><span><span class="language-xml"><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"space-y-6"</span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">header</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"space-y-2"</span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">p</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"text-xs font-medium uppercase tracking-[0.2em] text-indigo-400"</span></span><span>&gt;
          Genre
        </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">p</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">h1</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"text-2xl font-semibold tracking-tight sm:text-3xl"</span></span><span>&gt;
          Best {prettyGenre} Movies
        </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">h1</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">p</span></span></span><span> </span><span><span class="hljs-attr">className</span></span><span>=</span><span><span class="hljs-string">"max-w-xl text-xs text-slate-300"</span></span><span>&gt;
          A focused slice of the directory featuring {prettyGenre} titles. You
          can still search and sort within this subset.
        </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">p</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">header</span></span></span><span>&gt;

      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">MoviesList</span></span></span><span> </span><span><span class="hljs-attr">movies</span></span><span>=</span><span><span class="hljs-string">{movies}</span></span><span> </span><span><span class="hljs-attr">genres</span></span><span>=</span><span><span class="hljs-string">{allGenres}</span></span><span> /&gt;
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;
  );
}
</span></span></code></div></div></pre>
