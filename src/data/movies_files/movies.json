</span><span><span class="hljs-keyword">type</span></span><span> </span><span><span class="hljs-title class_">SortOption</span></span><span> = </span><span><span class="hljs-string">'rating-desc'</span></span><span> | </span><span><span class="hljs-string">'year-desc'</span></span><span> | </span><span><span class="hljs-string">'title-asc'</span></span><span>;

