</span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-attr">movies</span></span><span>: </span><span><span class="hljs-title class_">Movie</span></span><span>[] = moviesData </span><span><span class="hljs-keyword">as</span></span><span> </span><span><span class="hljs-title class_">Movie</span></span><span>[];

</span><span><span class="hljs-keyword">export</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">getAllMovies</span></span><span>(</span><span><span class="hljs-params"></span></span><span>): </span><span><span class="hljs-title class_">Movie</span></span><span>[] {
